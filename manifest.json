{
	"manifest_version": 3,
	"name": "DepressionTracker",
	"version": "0.0.1",
	"description": "Monitor and Treat Depression",
	"permissions": [
		"tabs",
		"storage",
		"downloads"
	],
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "./icon/icon_16.png",
		"32": "./icon/icon_32.png",
		"48": "./icon/icon_48.png",
		"128": "./icon/icon_128.png"
	},
	"action": {
		"default_popup": "./assets/popup.html",
	    "default_icon": {
	      "16": "./icon/icon_16.png",
	      "32": "./icon/icon_32.png",
	      "48": "./icon/icon_48.png",
	      "128": "./icon/icon_128.png"
		}
	}
}